{
  "name": "ngx-lottie",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-lottie/ngx-lottie.git"
  },
  "keywords": [
    "animation",
    "canvas",
    "svg",
    "after effects",
    "plugin",
    "angular"
  ],
  "author": "Artur Androsovych",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngx-lottie/ngx-lottie/issues"
  },
  "homepage": "https://github.com/ngx-lottie/ngx-lottie#readme",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "// - UTILS": "Utility scripts",
    "lottie:lint": "ng lint",
    "lottie:test": "ng test --silent",
    "lottie:build": "ts-node ./tools/build && ts-node ./tools/copy-readme",
    "format": "prettier --write \"{lottie,integration}/**/*.ts\"",
    "docs": "typedoc lottie/src --options typedoc.json",
    "// - CI": "CI pipelines",
    "ci:pipelines": "npm-run-all lottie:* && yarn integration:ng6",
    "// - RELEASE": "Scripts for version incrementing + generating changelog + releasing",
    "release": "git push origin && git push origin --tags",
    "release:major": "changelog -M && yarn release:lottie && yarn version --major && yarn release",
    "release:minor": "changelog -m && yarn release:lottie && yarn version --minor && yarn release",
    "release:patch": "changelog -p && yarn release:lottie && yarn version --patch && yarn release",
    "release:lottie": "git add . && git commit -m 'chore: update CHANGELOG.md'",
    "// - INTEGRATION": "Integration builds",
    "integration:ng6": "cd integration/lottie-ng6 && yarn test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "lottie-web": "5.4.4",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "7.3.1",
    "@angular-devkit/build-angular": "0.13.0",
    "@angular-devkit/build-ng-packagr": "0.13.0",
    "@angular/cli": "7.3.4",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-angular": "7.5.0",
    "@types/node": "11.10.4",
    "codelyzer": "4.5.0",
    "generate-changelog": "1.7.1",
    "husky": "1.3.1",
    "jest": "23.0.0",
    "lint-staged": "8.1.5",
    "ng-packagr": "4.7.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "ts-node": "7.0.0",
    "tsickle": "0.34.0",
    "tslib": "1.9.0",
    "tslint": "5.11.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-markdown": "1.1.27",
    "typescript": "3.1.6"
  }
}
